[["كَثْرَةُ الْعَجَلِ يُزِلُّ الْإِنْسَانَ","غرر الحكم ودرر الكلم"],["مَنْ عَجَّلَ زَلَّ","غرر الحكم ودرر الكلم"],["مَنْ رَكِبَ الْعَجَلَ أَدْرَكَ الزَّلَلَ","غرر الحكم ودرر الكلم"],["مَنْ رَكِبَ الْعَجَلَ كَبَا بِهِ الزَّلَلُ","غرر الحكم ودرر الكلم"],["مَعَ الْعَجَلِ يَكْثُرُ الزَّلَلُ","غرر الحكم ودرر الكلم"],["الْعَجَلُ يُوجِبُ الْعِثَارَ","غرر الحكم ودرر الكلم"],["إِيَّاكَ وَالْعَجَلَ فَإِنَّهُ مَقْرُونٌ بِالْعِثَارِ","غرر الحكم ودرر الكلم"],["ثَمَرَةُ الْعَجَلَةِ الْعِثَارُ","غرر الحكم ودرر الكلم"],["رَاكِبُ الْعَجَلِ مُشْفٍ [مُشْرِفٌ‏] عَلَى الْكَبْوَةِ","غرر الحكم ودرر الكلم"],["فِي الْعَجَلِ عِثَارٌ","غرر الحكم ودرر الكلم"],["مَنْ يَعْجَلْ يَعْثُرْ","غرر الحكم ودرر الكلم"],["مَنْ عَجِلَ كَثُرَ عِثَارُهُ","غرر الحكم ودرر الكلم"]]