[["رُبَّ عَالِمٍ قَتَلَهُ عِلْمُهُ","غرر الحكم ودرر الكلم"],["رُبَّ مُدَّعٍ لِلْعِلْمِ لَيْسَ بِعَالِمٍ","غرر الحكم ودرر الكلم"],["رُبَّ عَالِمٍ غَيْرُ مُنْتَفِعٍ","غرر الحكم ودرر الكلم"],["رُبَّمَا أَخْطَأَ الْبَصِيرُ رُشْدَهُ","غرر الحكم ودرر الكلم"],["مَنْ زَادَ عِلْمُهُ عَلَى عَقْلِهِ كَانَ وَبَالًا عَلَيْهِ","غرر الحكم ودرر الكلم"]]