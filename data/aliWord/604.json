[["السَّخَاءُ سَتْرُ الْعُيُوبِ","غرر الحكم ودرر الكلم"],["بِالسَّخَاءِ تُسْتَرُ الْعُيُوبُ","غرر الحكم ودرر الكلم"],["بِالْإِفْضَالِ تُسْتَرُ الْعُيُوبُ","غرر الحكم ودرر الكلم"],["غِطَاءُ الْعُيُوبِ السَّخَاءُ وَالْعَفَافُ","غرر الحكم ودرر الكلم"],["غَطُّوا مَعَايِبَكُمْ بِالسَّخَاءِ فَإِنَّهُ سَتْرُ الْعُيُوبِ","غرر الحكم ودرر الكلم"]]