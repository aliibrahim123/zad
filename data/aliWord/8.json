[["الْعِلْمُ مُمِيتُ الْجَهْلِ","غرر الحكم ودرر الكلم"],["الْعِلْمُ قَاتِلُ الْجَهْلِ","غرر الحكم ودرر الكلم"],["الْعِلْمُ قَاتِلُ الْجَهْلِ وَمُكْسِبُ النُّبْلِ","غرر الحكم ودرر الكلم"],["رُدُّوا الْجَهْلَ بِالْعِلْمِ","غرر الحكم ودرر الكلم"],["ضَادُّوا الْجَهْلَ بِالْعِلْمِ","غرر الحكم ودرر الكلم"],["مَنْ قَاتَلَ جَهْلَهُ بِعِلْمِهِ فَازَ بِالْحَظِّ الْأَسْعَدِ","غرر الحكم ودرر الكلم"],["يَسِيرُ الْعِلْمِ يَنْفِي كَثِيرَ الْجَهْلِ","غرر الحكم ودرر الكلم"]]