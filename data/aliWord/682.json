[["الْأُمُورُ بِالتَّجْرِبَةِ","غرر الحكم ودرر الكلم"],["الْأُمُورُ أَشْبَاهٌ [أَشْتَاتٌ‏]","غرر الحكم ودرر الكلم"],["التَّجَارِبُ لَا تَنْقَضِي","غرر الحكم ودرر الكلم"],["التَّجَارِبُ لَا تَنْقَضِي وَالْعَاقِلُ مِنْهَا فِي زِيَادَةٍ","غرر الحكم ودرر الكلم"],["الْأَيَّامُ تُفِيدُ التَّجَارِبَ","غرر الحكم ودرر الكلم"],["الْعَقْلُ حِفْظُ التَّجَارِبِ","غرر الحكم ودرر الكلم"],["حِفْظُ التَّجَارِبِ رَأْسُ الْعَقْلِ","غرر الحكم ودرر الكلم"],["الْحَزْمُ حِفْظُ التَّجْرِبَةِ","غرر الحكم ودرر الكلم"],["مَنْ حَفِظَ التَّجَارِبَ أَصَابَتْ أَفْعَالُهُ","غرر الحكم ودرر الكلم"],["مِنَ الْحَزْمِ حِفْظُ التَّجْرِبَةِ","غرر الحكم ودرر الكلم"],["الْمُجَرِّبُ أَحْكَمُ مِنَ الطَّبِيبِ","غرر الحكم ودرر الكلم"],["رَأْيُ الرَّجُلِ عَلَى قَدْرِ تَجْرِبَتِهِ","غرر الحكم ودرر الكلم"],["لَيْسَ كُلُّ مَنْ رَمَى يُصِيبُ","غرر الحكم ودرر الكلم"]]