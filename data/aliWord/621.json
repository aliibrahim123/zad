[["الْقَنَاعَةُ عِزٌّ","غرر الحكم ودرر الكلم"],["الْقَنَاعَةُ أَبْقَى عِزٍّ","غرر الحكم ودرر الكلم"],["الْقَنَاعَةُ عِزٌّ وَغَنَاءٌ","غرر الحكم ودرر الكلم"],["الْقَنَاعَةُ تُؤَدِّي إِلَى الْعِزِّ","غرر الحكم ودرر الكلم"],["اقْنَعْ تَعِزَّ","غرر الحكم ودرر الكلم"],["إِنْ تَقْنَعْ تَعِزَّ","غرر الحكم ودرر الكلم"],["بِالْقَنَاعَةِ يَكُونُ الْعِزُّ","غرر الحكم ودرر الكلم"],["ثَمَرَةُ الْقَنَاعَةِ الْعِزُّ","غرر الحكم ودرر الكلم"],["عِزُّ الْقُنُوعِ خَيْرٌ مِنْ ذُلِّ الْخُضُوعِ","غرر الحكم ودرر الكلم"],["غِنَى الْعَاقِلِ بِحِكْمَتِهِ وَعِزُّهُ بِقَنَاعَتِهِ","غرر الحكم ودرر الكلم"],["قَدْ عَزَّ مَنْ قَنِعَ","غرر الحكم ودرر الكلم"],["كَفَى بِالْقَنَاعَةِ مُلْكاً","غرر الحكم ودرر الكلم"],["مَنْ قَنِعَتْ نَفْسُهُ عَزَّ مُعْسِراً","غرر الحكم ودرر الكلم"],["مَنْ قَنِعَ كُفِيَ مَذَلَّةَ الطَّلَبِ","غرر الحكم ودرر الكلم"],["مَنْ كَثُرَ قُنُوعُهُ، قَلَّ خُضُوعُهُ","غرر الحكم ودرر الكلم"],["مَنْ قَنِعَ عَزَّ وَاسْتَغْنَى","غرر الحكم ودرر الكلم"],["مَنْ تَجَلْبَبَ الصَّبْرَ وَالْقَنَاعَةَ عَزَّ وَنَبُلَ","غرر الحكم ودرر الكلم"],["مَنْ أُتْحِفَ [الْتَحَفَ‏] الْعِفَّةَ وَالْقَنَاعَةَ حَالَفَهُ الْعِزُّ","غرر الحكم ودرر الكلم"],["مِنْ عِزِّ النَّفْسِ لُزُومُ الْقَنَاعَةِ","غرر الحكم ودرر الكلم"],["نَالَ الْعِزَّ مَنْ رُزِقَ الْقَنَاعَةَ","غرر الحكم ودرر الكلم"],["لَا أَعَزَّ مِنْ قَانِعٍ","غرر الحكم ودرر الكلم"]]