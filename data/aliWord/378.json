[["الْأَدَبُ أَفْضَلُ حَسَبٍ","غرر الحكم ودرر الكلم"],["أَشْرَفُ حَسَبٍ حُسْنُ أَدَبٍ","غرر الحكم ودرر الكلم"],["أَكْرَمُ حَسَبٍ حُسْنُ الْأَدَبِ","غرر الحكم ودرر الكلم"],["إِنَّمَا الشَّرَفُ بِالْعَقْلِ وَالْأَدَبِ لَا بِالْمَالِ وَالْحَسَبِ","غرر الحكم ودرر الكلم"],["حُسْنُ الْأَدَبِ يَسْتُرُ قُبْحَ النَّسَبِ","غرر الحكم ودرر الكلم"],["حُسْنُ الْأَدَبِ أَفْضَلُ نَسَبٍ وَأَشْرَفُ سَبَبٍ","غرر الحكم ودرر الكلم"],["حَسَبُ الْأَدَبِ أَشْرَفُ مِنْ حَسَبِ النَّسَبِ","غرر الحكم ودرر الكلم"],["طَلَبُ الْأَدَبِ جَمَالُ الْحَسَبِ","غرر الحكم ودرر الكلم"],["عَلَيْكَ بِالْأَدَبِ فَإِنَّهُ زَيْنُ الْحَسَبِ","غرر الحكم ودرر الكلم"],["قَلِيلُ الْأَدَبِ خَيْرٌ مِنْ كَثِيرِ النَّسَبِ","غرر الحكم ودرر الكلم"],["مَنْ قَعَدَ بِهِ حَسَبُهُ نَهَضَ بِهِ أَدَبُهُ","غرر الحكم ودرر الكلم"],["مُرُوَّةُ الْعَاقِلِ دِينُهُ وَحَسَبُهُ أَدَبُهُ","غرر الحكم ودرر الكلم"],["نِعْمَ النَّسَبُ حُسْنُ الْأَدَبِ","غرر الحكم ودرر الكلم"],["لَا حَسَبَ كَالْأَدَبِ","غرر الحكم ودرر الكلم"],["لَا حَسَبَ أَرْفَعُ مِنَ الْأَدَبِ","غرر الحكم ودرر الكلم"]]